{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: Current File (Integrated Terminal)",
        "type": "python",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal",
        "justMyCode": false
      },
      {
        "name": "Python: Attach using Process Id",
        "type": "python",
        "request": "attach",
        "processId": "${command:pickProcess}"
      },
      {
        "name": "Jupyter: Debug Current File",
        "type": "jupyter",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal",
        "justMyCode": false
      }
    ]
  }